    "web-vitals": "^1.1.0",
    "workbox-background-sync": "^5.1.4",
    "workbox-broadcast-update": "^5.1.4",
    "workbox-cacheable-response": "^5.1.4",
    "workbox-core": "^5.1.4",
    "workbox-expiration": "^5.1.4",
    "workbox-google-analytics": "^5.1.4",
    "workbox-navigation-preload": "^5.1.4",
    "workbox-precaching": "^5.1.4",
    "workbox-range-requests": "^5.1.4",
    "workbox-routing": "^5.1.4",
    "workbox-strategies": "^5.1.4",
    "workbox-streams": "^5.1.4"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.0.6",
    "etag": "^1.8.1",
    "express": "^4.17.1",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/ --recursive",
    "watch-css": "node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-dev-express": "cross-env NODE_ENV=dev node service.js",
    "start-prod-express": "cross-env NODE_ENV=production node service.js",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js start-dev-express",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "./node_modules/.bin/mocha --require @babel/register",
    "eject": "react-scripts eject"
  },
  "mocha": {
    "require": [
      "@babel/register"
    ]
  },
  "prettier": {
    "useTabs": true,
    "singleQuote": true,
    "bracketSpacing": false,
    "arrowParens": "always",
    "semi": true,
    "trailingComma": "all",
    "printWidth": 160
  },
  "proxy": "http://localhost:3001",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
