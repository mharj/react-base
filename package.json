    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-react": "^7.22.0",
    "i18next": "^19.8.4",
    "i18next-browser-languagedetector": "^6.0.1",
    "mocha": "^8.2.1",
    "node-sass": "^5.0.0",
    "node-sass-chokidar": "^1.5.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.8.5",
