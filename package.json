{
  "name": "react-base",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.13.0",
    "eslint-plugin-jsx": "0.0.2",
    "eslint-plugin-react": "^7.13.0",
    "i18next": "^17.0.2",
    "i18next-browser-languagedetector": "^3.0.1",
    "node-sass": "^4.12.0",
    "node-sass-chokidar": "^1.3.5",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-dom": "^16.8.6",
    "react-helmet": "beta",
    "react-i18next": "^10.11.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1",
    "react-test-renderer": "^16.8.6",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.5.3",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0"
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "cross-fetch": "^3.0.3",
    "etag": "^1.8.1",
    "express": "^4.17.1",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/ --recursive",
    "watch-css": "node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-dev-express": "cross-env NODE_ENV=dev node service.js",
    "start-prod-express": "cross-env NODE_ENV=production node service.js",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js start-dev-express",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "prettier": {
    "useTabs": true,
    "singleQuote": true,
    "bracketSpacing": false,
    "arrowParens": "always",
    "semi": true,
    "trailingComma": "all",
    "printWidth": 160
  },
  "proxy": "http://localhost:3001",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
